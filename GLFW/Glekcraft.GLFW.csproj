<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="https://github.com/dotnet/msbuild/raw/main/src/MSBuild/MSBuild/Microsoft.Build.Core.xsd" type="application/xml" schematypens="http://schemas.microsoft.com/developer/msbuild/2003"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <RestorePackagesWithLockFile>true</RestorePackagesWithLockFile>
        <RootNamespace>Glekcraft.GLFW</RootNamespace>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>disable</ImplicitUsings>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
        <NoWarn>IDE0130</NoWarn>
    </PropertyGroup>
    <ItemGroup>
        <Using Include="System" />
    </ItemGroup>
    <Import Condition="Exists('$(MSBuildThisFileDirectory)../UserNatives.props') == 'true' AND $(UserNativesProps) == ''" Project="$(MSBuildThisFileDirectory)../UserNatives.props" />
    <Import Condition="Exists('$(MSBuildThisFileDirectory)../Natives.props') == 'true' AND $(NativesProps) == ''" Project="$(MSBuildThisFileDirectory)../Natives.props" />
    <ItemGroup>
        <Content Include="$(MSBuildThisFileDirectory)native/$(Natives_TargetRuntimeIdentifier)/$(Natives_TargetRuntimeLibraryPrefix)glfw$(Natives_TargetRuntimeLibrarySuffix)$(Natives_TargetRuntimeLibraryExtension)">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
            <Visible>false</Visible>
            <Link>$(Natives_TargetRuntimeLibraryPrefix)glfw$(Natives_TargetRuntimeLibrarySuffix)$(Natives_TargetRuntimeLibraryExtension)</Link>
        </Content>
    </ItemGroup>
</Project>
